{
    "compilerOptions": {
      "baseUrl": "./src",
      "noEmit": true,
      "target": "es2022",
      "noImplicitAny": false,
      "jsx": "preserve",
      "module": "esnext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports" : true,
      "allowJs": true,
      "types": ["vite/client"],
      "moduleResolution": "node",
  }
}